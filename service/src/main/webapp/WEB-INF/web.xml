<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <filter>
        <filter-name>MasterFilter</filter-name>
        <filter-class>com.jiechic.android.architecutre.service.filter.MasterFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>MasterFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>


    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.jiechic.android.architecutre.service.servlet.RegisterServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.jiechic.android.architecutre.service.servlet.LoginServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>goods</servlet-name>
        <servlet-class>com.jiechic.android.architecutre.service.servlet.GoodsServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>goods</servlet-name>
        <url-pattern>/goods</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addgood</servlet-name>
        <servlet-class>com.jiechic.android.architecutre.service.servlet.AddGoodServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>addgood</servlet-name>
        <url-pattern>/addgood</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>good</servlet-name>
        <servlet-class>com.jiechic.android.architecutre.service.servlet.GoodServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>good</servlet-name>
        <url-pattern>/good</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>userinfo</servlet-name>
        <servlet-class>com.jiechic.android.architecutre.service.servlet.UserInfoServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>userinfo</servlet-name>
        <url-pattern>/userinfo</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateuserinfo</servlet-name>
        <servlet-class>com.jiechic.android.architecutre.service.servlet.UpdateUserInfoServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateuserinfo</servlet-name>
        <url-pattern>/updateuserinfo</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updategood</servlet-name>
        <servlet-class>com.jiechic.android.architecutre.service.servlet.UpdateGoodServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>updategood</servlet-name>
        <url-pattern>/updategood</url-pattern>
    </servlet-mapping>
</web-app>